{
  "bindings": [
    {
      "type": "cosmosDBTrigger",
      "name": "itemIn",
      "direction": "in",
      "leaseCollectionName": "leases",
      "connectionStringSetting": "AzureWebJobsCosmosDBConnectionString",
      "databaseName": "ItemDb",
      "collectionName": "ItemCollectionIn$E2EResourceSuffix$",
      "createLeaseCollectionIfNotExists": true
    },
    {
      "type": "cosmosDB",
      "name": "$return",
      "direction": "out",
      "leaseCollectionName": "leases",
      "connectionStringSetting": "AzureWebJobsCosmosDBConnectionString",
      "databaseName": "ItemDb",
      "collectionName": "ItemCollectionOut$E2EResourceSuffix$"
    }
  ]
}